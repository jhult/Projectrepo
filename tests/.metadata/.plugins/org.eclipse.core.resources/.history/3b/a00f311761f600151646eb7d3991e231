 /*-----Navigation to CPQ Page-----*/
	       
	       
	  	     driver.navigate().to("https://cpqau.bigmachines.com/sso/saml_request.jsp");
	  	       driver.manage().timeouts().implicitlyWait(Config.WAIT_FOR_SECONDS, TimeUnit.SECONDS);
