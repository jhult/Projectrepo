                           driver.findElement(CloudHomePage.Home_Manage_Resources).click();
                           driver.manage().timeouts().implicitlyWait(Config.WAIT_FOR_SECONDS, TimeUnit.SECONDS);
                           Thread.sleep(10000);
                           driver.manage().timeouts().pageLoadTimeout(10000, TimeUnit.SECONDS);
                           
                           
                           driver.findElement(CloudHomePage.HOME_RESOURCE_FAQ).click();
                           driver.manage().timeouts().implicitlyWait(Config.WAIT_FOR_SECONDS, TimeUnit.SECONDS);
                         
                           Assert.assertEquals(driver.findElement(CloudHomePage.HOME_RESOURCE_FAQ_TITLE) .getText(), "General");
     //                      System.out.println("FAQ ");
                           Thread.sleep(5000);
                           driver.manage().timeouts().pageLoadTimeout(10000, TimeUnit.SECONDS);
                           
      //                   ----- Home Page, Resource link, Press Release link ------
                           
                           driver.findElement(CloudHomePage.Home_Manage_Resources).click();

                           driver.manage().timeouts().implicitlyWait(Config.WAIT_FOR_SECONDS, TimeUnit.SECONDS);

                           Thread.sleep(10000);
                           driver.manage().timeouts().pageLoadTimeout(10000, TimeUnit.SECONDS);
                           
                           
                           driver.findElement(CloudHomePage.HOME_RESOURCE_PRESS_RELEASE).click();

                           driver.manage().timeouts().implicitlyWait(Config.WAIT_FOR_SECONDS, TimeUnit.SECONDS);
                           Thread.sleep(5000);
                           driver.manage().timeouts().pageLoadTimeout(10000, TimeUnit.SECONDS);
                           
     //                       ----- Navigating to New Window -------------------------------------------

                           Set allWindowHandles1 = driver.getWindowHandles();
                           int win2 = allWindowHandles1.size();
                           System.out.println("no. of windows open  "  + win2);
                           Thread.sleep(5000);
                           String window2 = (String) allWindowHandles1.toArray()[2];
